{
  "name": "opencommit",
  "id": "opencommit",
  "version": "0.0.10",
  "description": "opencommit using ollama",
  "installsAfter": [
    "ghcr.io/devcontainers/features/common-utils",
    "ghcr.io/devcontainers/features/node"
  ],
  "containerEnv": {
    "OCO_AI_PROVIDER": "ollama"
  },
  "onCreateCommand": "oco hook set || continue"
}
